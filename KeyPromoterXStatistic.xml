<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Build Project">
        <Item>
          <IdeaActionID>CompileDirty</IdeaActionID>
          <ShortCut>'Strg+F9'</ShortCut>
          <Description>Build Project</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Strg+C' or 'Strg+Einfg'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project Structure...">
        <Item>
          <IdeaActionID>ShowProjectStructureSettings</IdeaActionID>
          <ShortCut>'Strg+Alt+Umschalt+S'</ShortCut>
          <Description>Project Structure...</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'All Tests'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'Strg+Umschalt+F10'</ShortCut>
          <Description>Run 'All Tests'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'All in bowling_test'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Umschalt+F10'</ShortCut>
          <Description>Run 'All in bowling_test'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Strg+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>